<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="MyGtkAppWindow" parent="GtkApplicationWindow">
    <property name="title">My GTK App</property>
    <child>
      <object class="GtkBox" id="box1">
        <property name="orientation">vertical</property>
        <child>
          <object class="MyGtkAppCustomButton" id="button">
            <signal name="clicked" handler="handle_button_clicked" />
            <property name="label">Press me!</property>
            <property name="margin-top">12</property>
            <property name="margin-bottom">12</property>
            <property name="margin-start">12</property>
            <property name="margin-end">12</property>
          </object>
        </child>
        <child>
          <object class="MyGtkAppCustomButton" id="button2">
            <signal name="clicked" handler="handle_button2_clicked" />
            <property name="label">Welldone</property>
            <property name="margin-top">12</property>
            <property name="margin-bottom">12</property>
            <property name="margin-start">12</property>
            <property name="margin-end">12</property>
          </object>
        </child>
        <child>
          <object class="MyGtkAppCustomButton" id="button3">
            <signal name="clicked" handler="handle_button3_clicked" />
            <property name="label">Close</property>
            <property name="margin-top">12</property>
            <property name="margin-bottom">12</property>
            <property name="margin-start">12</property>
            <property name="margin-end">12</property>
          </object>
        </child>
        <child>
          <object class="MyGtkAppCustomButton" id="button4">
            <signal name="clicked" handler="handle_button4_clicked" swapped="true" />
            <property name="label">0</property>
            <property name="margin-top">12</property>
            <property name="margin-bottom">12</property>
            <property name="margin-start">12</property>
            <property name="margin-end">12</property>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>